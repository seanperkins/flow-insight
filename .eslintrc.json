{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "globals": {
    "document": false,
    "module": false,
    "process": false,
    "require": false,
    "__dirname": false,
    "window": false
  },
  "rules": {
    "no-console": "off",
    "react/prop-types": "off",
    "no-unused-vars": "off"
  }
}